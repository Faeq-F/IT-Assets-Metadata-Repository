{
	"recurseDepth": 10,
	"source": {
		"includePattern": ".(cjs|js)$",
		"excludePattern": "(^|\\/|\\\\)_",
		"include": ["src"],
		"exclude": ["node_modules"]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
