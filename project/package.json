		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@vitest/coverage-v8": "^1.2.1",
		"c8": "^9.1.0",
		"cors": "^2.8.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
